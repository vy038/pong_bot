/* Classes */

// Creating a new class that inherits from the ESP_NOW_Peer class is required.

class ESP_NOW_Broadcast_Peer : public ESP_NOW_Peer {
public:
  // Constructor of the class using the broadcast address
  ESP_NOW_Broadcast_Peer(uint8_t channel, wifi_interface_t iface, const uint8_t *lmk) : ESP_NOW_Peer(ESP_NOW.BROADCAST_ADDR, channel, iface, lmk) {}

  // Destructor of the class
  ~ESP_NOW_Broadcast_Peer() {
    remove();
  }

  // Function to properly initialize the ESP-NOW and register the broadcast peer
  bool begin() {
    if (!ESP_NOW.begin() || !add()) {
      log_e("Failed to initialize ESP-NOW or register the broadcast peer");
      return false;
    }
    return true;
  }

  // Function to send a message to all devices within the network
  bool send_message(const uint8_t *data, size_t len) {
    if (!send(data, len)) {
      log_e("Failed to broadcast message");
      return false;
    }
    return true;
  }
};

// Create a broadcast peer object
ESP_NOW_Broadcast_Peer broadcast_peer(ESPNOW_WIFI_CHANNEL, WIFI_IF_STA, NULL);

/* Main */

void setup() {
  Serial.begin(115200);

  pinMode(buttonA, INPUT_PULLUP);
  pinMode(buttonB, INPUT_PULLUP);
  pinMode(joyX, INPUT_PULLUP);
  pinMode(joyY, INPUT_PULLUP);

  Wire.begin(22, 23); // SDA, SCK

  // OLED STUFF
  delay(250); // wait for the OLED to power up
  display.begin(i2c_Address, true); // Address 0x3C default
  // display.setContrast (0); // dim display
  display.display();
  delay(2000);
  // Clear the buffer.
  display.clearDisplay();

  // Initialize the Wi-Fi module
  WiFi.mode(WIFI_STA);
  WiFi.setChannel(ESPNOW_WIFI_CHANNEL);
  while (!WiFi.STA.started()) {
    delay(100);
  }

  Serial.println("ESP-NOW - Controller Master for Pong");
  Serial.println("Wi-Fi parameters:");
  Serial.println("  Mode: STA");
  Serial.println("  MAC Address: " + WiFi.macAddress());
  Serial.printf("  Channel: %d\n", ESPNOW_WIFI_CHANNEL);

  // Register the broadcast peer
  if (!broadcast_peer.begin()) {
    Serial.println("Failed to initialize broadcast peer");
    Serial.println("Reebooting in 5 seconds...");
    delay(5000);
    ESP.restart();
  }

  Serial.println("Setup complete. Broadcasting messages every 5 seconds.");

  show(epd_bitmap_manual);
}

void loop() {
  int joyYState = analogRead(joyX);
  int joyXState = analogRead(joyY);
  joyYState = map(joyYState, 0, 4095, 4, 0);
  joyXState = map(joyXState, 0, 4095, 4, 0);
  int buttonAState = digitalRead(buttonA);
  int buttonBState = digitalRead(buttonB);

  
  if (buttonAState == LOW && buttonBState == LOW) {  // change modes 
    if (mode == 0) {
      mode = 1;
      show(epd_bitmap_auto);
    } else {
      mode = 0;
      show(epd_bitmap_manual);
    }
    tone(speakerPin, 523.25, 750);
    delay(750);
  } else if (mode == 0) {
    if ((joyXState == 0 || joyXState == 4) && (joyYState != 0 && joyYState != 4)) {  // movement for true side
      side(joyXState, buttonBState);
    } else if ((joyYState == 0 || joyYState == 4) && (joyXState != 0 && joyXState != 4)) {  // movement for true forward/backward
      forwardFunc(joyYState);
    } else if ((joyYState == 0 || joyYState == 4) && (joyXState == 0 || joyXState == 4)) {  // diagonal
      if (buttonBState == LOW) {               // turn or
        side(joyXState, 1);
      } else {                                 // go diagonal
        diagonal(joyYState, joyXState);
      }
    } else {  // stop the bot
      show(epd_bitmap_manual);
    }
    if (buttonAState == LOW) {  // fire when a button is pressed
      tone(speakerPin, 261.63, 200);
      show(epd_bitmap_firing);
      delay(500);
    }
  } else if (mode == 1) {
    show(epd_bitmap_auto);
  } 

  // Broadcast a message to all devices within the network
  char data[16];
  snprintf(data, sizeof(data), "%d%d%d%d", joyYState, joyXState, buttonAState, buttonBState);

  Serial.printf("Broadcasting message: %s\n", data);

  if (!broadcast_peer.send_message((uint8_t *)data, sizeof(data))) {
    Serial.println("Failed to broadcast message");
  }

  delay(50);
}


// function to display a direction on OLED screen
void show(const uint8_t *bitmap) {
  display.clearDisplay();
  display.drawBitmap(0, 0, bitmap, 128, 64, SH110X_WHITE);
  display.display();
}

// move forward function (or backwards, depending on input variable)
void forwardFunc(int dir) {
  switch (dir) {
    case 0:  // backwards
      show(epd_bitmap_bottom);
      break;
    case 4:  // forwards
      show(epd_bitmap_top);
      break;
  }
}

// move left or right depending in input variable (0 is right, 1 is left)
void side(int dir, int buttonState) {
  if (buttonState == 0) { // turning
    switch (dir) {
      case 4:  // to the right
        show(epd_bitmap_turn_right);
        break;
      case 0:  // to the left
        show(epd_bitmap_turn_left);
        break;
    }
  } else { // side
    switch (dir) {
      case 4:  // to the right
        show(epd_bitmap_right);
        break;
      case 0:  // to the left
        show(epd_bitmap_left);
        break;
    }
  }
}

// move the bot in diagonal directions, using only two inputs
void diagonal(int Y, int X) {
  if (Y == 4) {  // either FL or FR
    switch (X) {
      case 4:  // to the right (FR)
        show(epd_bitmap_top_right);
        break;
      case 0:  // to the left (FL)
        show(epd_bitmap_top_left);
        break;
    }
  } else {  // either BL or BR
    switch (X) {
      case 4:  // to the right (BR)
        show(epd_bitmap_bottom_right);
        break;
      case 0:  // to the left (BL)
        show(epd_bitmap_bottom_left);
        break;
    }
  }
}
